<div class="content">
  <div class="container-fluid">
    <div class="row">
      <div class="col-lg-12 col-md-12">
        <div class="card">
          <div class="header">
            <h4 class="title">Upload File</h4>
            <% if (file) { %>
              <p style="color:red">Form Uploaded Successfully</p>
              <% } %>
          </div>
          <div class="content">
            <form name="form" enctype="multipart/form-data" method="post" action="/uploadfile">
              <div class="row">
                <div class="col-md-12">
                  <div class="form-group">
                    <label>Name</label>
                    <input id="uploadFile" name="uploadFile" type="file" />
                  </div>
                </div>
              </div>
              <div class="text-center">
                <button type="submit" class="btn btn-info btn-fill btn-wd">Add File</button>
              </div>
              <div class="clearfix"></div>
            </form>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
